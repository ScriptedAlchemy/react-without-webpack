{
  "name": "react-without-webpack",
  "version": "1.0.0",
  "main": "./server/entrypoint.js",
  "license": "MIT",
  "scripts": {
    "start": "./node_modules/.bin/pm2 start conf/process.json",
    "dev": "cross-env NODE_ENV=development MYAPP_ENV=local node ./server/entrypoint.js",
    "stage": "cross-env NODE_ENV=development MYAPP_ENV=staging node ./server/entrypoint.js",
    "clean": "./node_modules/.bin/rimraf public/js/vendors",
    "prepare": "./node_modules/.bin/mkdirp public/js/vendors",
    "build": "npm run clean && npm run prepare && node ./conf/vendors.js",
    "postinstall": "node ./conf/quickfix.js && npm run build ",
    "format": "./node_modules/.bin/prettier --write --no-semi --single-quote \"{,!(node_modules|public|middleware|conf)/**/}*.js\""
  },
  "dependencies": {
    "aphrodite": "^1.2.4",
    "d3": "^4.11.0",
    "history": "^4.7.2",
    "immutable": "^3.8.2",
    "lodash.debounce": "^4.0.8",
    "prop-types": "^15.6.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-helmet": "^5.2.0",
    "react-redux": "^5.0.6",
    "react-router-config": "^1.0.0-beta.4",
    "react-router-dom": "^4.2.2",
    "recompose": "^0.26.0",
    "redux": "^3.7.2",
    "redux-immutable": "^4.0.0",
    "redux-thunk": "^2.2.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.25.0",
    "babel-plugin-module-resolver": "^2.7.1",
    "babel-plugin-syntax-object-rest-spread": "^6.13.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.0",
    "babel-preset-react": "^6.24.1",
    "babel-register": "^6.26.0",
    "bluebird": "^3.5.0",
    "body-parser": "^1.17.2",
    "chokidar": "^1.7.0",
    "compression": "^1.7.0",
    "cookie-parser": "^1.4.3",
    "cross-env": "^5.0.5",
    "express": "^4.15.3",
    "glob": "^7.1.2",
    "isomorphic-fetch": "^2.2.1",
    "mkdirp": "^0.5.1",
    "morgan": "^1.8.2",
    "njwt": "^0.4.0",
    "path": "^0.12.7",
    "pm2": "^2.6.1",
    "prettier": "^1.7.0",
    "rimraf": "^2.6.2",
    "rollup": "^0.43.0",
    "rollup-plugin-commonjs": "^8.0.2",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-replace": "^1.1.1",
    "shelljs": "^0.7.8",
    "spdy": "^3.4.7",
    "vhost": "^3.0.2"
  }
}
